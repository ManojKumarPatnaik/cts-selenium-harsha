<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!--Here test count = 4 runs 2 test cases in each progam at a time to save time and later it uns remining 2 tests also parallelly-->
<suite  thread-count="4" name="Suite" parallel = "methods">
  <test thread-count="4" name="Test" parallel = "methods">
  
    <classes>
      <class name="cts.testng.harsha.ParallelTestCases"/>
      <class name="cts.testng.harsha.ParallelTestCases2"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
